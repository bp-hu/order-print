{
  "name": "order-print",
  "version": "1.0.0",
  "scripts": {
    "reset": "npx rimraf ./**/node_modules",
    "setup": "pnpm run reset && pnpm install",
    "dev": "edenx dev",
    "build": "edenx build",
    "start": "edenx start",
    "serve": "edenx serve",
    "new": "edenx new",
    "lint": "biome check",
    "deploy": "edenx deploy",
    "deploy:preview": "edenx deploy --preview",
    "prepare": "simple-git-hooks",
    "upgrade": "edenx upgrade"
  },
  "engines": {
    "node": ">=20"
  },
  "lint-staged": {
    "*.{js,ts,cjs,mjs,d.cts,d.mts,jsx,tsx,json,jsonc}": [
      "biome check --files-ignore-unknown=true"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "dependencies": {
    "@auix/theme": "^0.1.10",
    "@auix/utils": "^0.0.8",
    "@douyinfe/semi-icons": "^2.91.0",
    "@douyinfe/semi-illustrations": "^2.91.0",
    "@douyinfe/semi-ui": "^2.91.0",
    "@edenx/runtime": "1.68.14",
    "@safe-fe/hooks": "^3.1.4",
    "@safe-fe/utils": "^2.0.5",
    "@tailwindcss/postcss": "^4.1.18",
    "jotai": "^2.17.1",
    "localforage": "^1.10.0",
    "qs": "^6.15.0",
    "react": "~18.3.0",
    "react-dom": "~18.3.0",
    "react-draggable": "^4.5.0",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@douyinfe/semi-rspack-plugin": "^2.91.0",
    "@edenx/app-tools": "1.68.14",
    "@edenx/proxy": "^3.0.3",
    "@edenx/tsconfig": "1.68.14",
    "@types/node": "^20",
    "@types/qs": "^6.14.0",
    "@types/react": "~18.3.12",
    "@types/react-dom": "~18.3.1",
    "lint-staged": "~13.1.0",
    "prettier": "^3.4.2",
    "rimraf": "~3.0.2",
    "simple-git-hooks": "^2.11.1",
    "typescript": "~5.7.3"
  }
}